@model List<CategoryViewModel>
@{
    Layout = null;
}

<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
<table class="table table-bordered">
  
    <tr>
        <th>#</th>
        <th>Kategori Adı</th>

    </tr>
    @foreach(var item in Model){

        <tr>
            <td>@item.CategoryId</td>
            <td>@item.CategoryName</td>
        </tr>
    }
</table>
